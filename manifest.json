{
  "name": "CrypTick",
  "description": "Cryptocurrency Market Data",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_popup": "coins.html",
    "default_icon": "images/icon16.png"
  },
  "permissions": [
    "storage",
    "notifications",
    "activeTab",
    "<all_urls>",
    "https://*/*"
  ]
}
